{
   "name": "unifarm-token-beneficiaries-list",
   "version": "1.0.0",
   "main": "./dist/server.js",
   "license": "MIT",
   "scripts": {
      "dev": "export NODE_ENV=dev && nodemon --ext '.ts' server",
      "create:json": "ts-node ./scripts/index.ts",
      "clean": "rimraf ./dist",
      "killport": "kill-port --port 3000",
      "build": "babel ./ --extensions '.ts,.js' --out-dir ./dist --config-file ./.babelrc --copy-files && rm -rf ./dist/node_modules ./dist/package.json ./dist/tsconfig.json",
      "server": "pm2 start -f ./dist/server.js",
      "start": "export NODE_ENV=prod && npm-run-all clean killport build server",
      "create:database": "sequelize db:create",
      "migrations": "sequelize db:migrate:undo:all && sequelize db:migrate",
      "seeders": "sequelize db:seed:all"
   },
   "dependencies": {
      "@ethersproject/address": "^5.1.0",
      "@types/sequelize": "^4.28.9",
      "@types/validator": "^13.1.3",
      "ajv": "^8.2.0",
      "babel-preset-es2015": "^6.24.1",
      "body-parser": "^1.19.0",
      "concurrently": "^6.0.2",
      "cors": "^2.8.5",
      "dayjs": "^1.10.4",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-rate-limit": "^5.2.6",
      "express-validation": "^3.0.8",
      "express-validator": "^6.11.1",
      "fs-extra": "^10.0.0",
      "fuse.js": "^6.4.6",
      "helmet": "^4.6.0",
      "import-modules": "^2.1.0",
      "kill-port": "^1.6.1",
      "lodash": "^4.17.21",
      "morgan": "^1.10.0",
      "npm-run-all": "^4.1.5",
      "pg": "^8.6.0",
      "pm2": "^4.5.6",
      "postgres": "^1.0.2",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "sequelize": "^6.6.2",
      "sequelize-typescript": "^2.1.0",
      "validator": "^13.6.0",
      "web3-utils": "^1.3.5",
      "winston": "^3.3.3"
   },
   "devDependencies": {
      "@babel/cli": "^7.13.16",
      "@babel/core": "^7.14.0",
      "@babel/node": "^7.13.13",
      "@babel/plugin-transform-runtime": "^7.14.2",
      "@babel/plugin-transform-modules-commonjs": "^7.14.0",
      "@babel/preset-env": "^7.14.1",
      "@babel/preset-typescript": "^7.13.0",
      "@types/express": "^4.17.11",
      "@types/node": "^15.0.1",
      "@typescript-eslint/eslint-plugin": "^4.22.0",
      "babel-plugin-inline-json-import": "^0.3.2",
      "prettier": "^2.2.1",
      "sequelize-cli": "^6.2.0",
      "ts-node": "^9.1.1",
      "typescript": "^4.2.4"
   }
}
