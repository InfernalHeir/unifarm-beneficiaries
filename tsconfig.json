{
   "compilerOptions": {
      "target": "ES2015",
      "module": "commonjs",
      "allowJs": true,
      "isolatedModules": false,
      "strict": true,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
   },
   "exclude": ["node_modules", "./dist", "seeders", "migrations", "config", "models"]
}
